{
  "version": "0.2.0",
  "configurations": [{
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/workspaces/LitLaunch/LitLaunch-C/Run",
      "program": "${workspaceFolder}/LitLaunch-C/litlaunch-library-api/bin/litlaunch-library-api.so",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}